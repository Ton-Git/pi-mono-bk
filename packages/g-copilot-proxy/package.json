{
  "name": "g-copilot-proxy",
  "version": "1.0.0",
  "description": "OpenAI & Anthropic compatible proxy server for GitHub Copilot",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsgo -p tsconfig.json --watch",
    "build": "tsgo -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "check": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "@hono/node-server": "^1.14.1",
    "@mariozechner/pi-ai": "^0.52.7",
    "dotenv": "^16.4.7",
    "hono": "^4.6.16",
    "uuid": "^11.0.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
